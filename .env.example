# Yandex Cloud API (обязательно)
YANDEX_API_KEY=your_yandex_api_key_here
YANDEX_FOLDER_ID=your_yandex_folder_id_here

# MCP Weather Tools (опционально)
# Получить ключ можно на https://yandex.ru/dev/weather/
# Если не указан - функция получения погоды через MCP будет недоступна
YANDEX_WEATHER_API_KEY=your_yandex_weather_api_key_here

# Опционально: OpenRouter API для сравнения моделей
# Получить ключ можно на https://openrouter.ai/
# Если не указан - функция сравнения моделей будет недоступна
OPENROUTER_API_KEY=your_openrouter_api_key_here
